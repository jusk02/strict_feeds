<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<%if  @current_user %>

<% @current_user.google.files.each do |file| %>

  <% if file.resource_type == "folder" %>
    <%= file.name %><br>
  <% end %>
<% end %>



<%else  %>

<% provide(:title, "Log in") %>
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path, :html => {:target=>'_blank'}) do |f| %>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>


  </div>
</div>
<%end %>

